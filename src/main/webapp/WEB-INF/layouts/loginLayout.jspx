	<script src="${res}/js/bootstrap-popover.js"><!-- eh --></script>
	<script src="${res}/js/bootstrap-button.js"><!-- eh --></script>
	<script src="${res}/js/bootstrap-collapse.js"><!-- eh --></script>
	<script src="${res}/js/bootstrap-carousel.js"><!-- eh --></script>
	<script src="${res}/js/bootstrap-typeahead.js"><!-- eh --></script>
</html>